{
    "manifest_version": 2,
    "name": "YParéoToCSV",
    "version": "0.0.1",
    "description": "Permet de télécharger un bilan annuel en version CSV (utilisant des ; comme séparateurs)",
    "icons": {
        "32": "logo32.png"
    },
    "browser_action": {
        "default_icon": {
            "32": "logo32.png"
        },
        "default_title": "Exécuter le script"
    },
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": ["background.js"]
    }
}